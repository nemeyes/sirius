<UserControl x:Class="sirius.app.server.arbitrator.Settings.Settings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab SelectedSource="/Settings/NetworkSettings.xaml" Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Network" Source="/Settings/NetworkSettings.xaml" />
                <!--mui:Link DisplayName="Media" Source="/Settings/MediaSettings.xaml" //-->
                <mui:Link DisplayName="Attendants" Source="/Settings/AttendantsSettings.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>

</UserControl>
